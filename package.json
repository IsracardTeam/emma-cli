{
  "name": "emma-cli",
  "description": "Great assistant who helps you search Yarn and NPM in your terminal.",
  "author": "Matic Zavadlal <matic.zavadlal@gmail.com>",
  "version": "0.0.0-semantic-release",
  "bin": {
    "emma": "dist/index.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "release": {
    "branch": "master"
  },
  "scripts": {
    "build": "babel src --out-dir dist --ignore __tests__",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "dev": "babel -w src --out-dir dist --ignore __tests__",
    "test": "xo && ava",
    "pretest": "npm run build",
    "semantic-release": "semantic-release"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/maticzav/emma-cli",
  "keywords": [
    "emma"
  ],
  "bugs": {
    "url": "https://github.com/maticzav/emma-cli/issues"
  },
  "dependencies": {
    "algoliasearch": "^3.24.12",
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link": "^1.2.1",
    "apollo-link-http": "^1.5.3",
    "apollo-link-ws": "^1.0.7",
    "apollo-utilities": "^1.0.11",
    "conf": "^1.4.0",
    "dot-prop": "^4.2.0",
    "execa": "^0.9.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "ink": "^0.4.1",
    "ink-link": "^0.2.0",
    "ink-spinner": "^1.0.0",
    "ink-text-input": "^1.1.1",
    "isomorphic-fetch": "^2.2.1",
    "meow": "^4.0.0",
    "opn": "^5.3.0",
    "subscriptions-transport-ws": "^0.9.7",
    "term-size": "^1.2.0",
    "update-notifier": "^2.3.0",
    "ws": "^5.1.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.40",
    "ava": "^0.25.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "cross-env": "^5.1.3",
    "prettier": "^1.11.1",
    "semantic-release": "^12.4.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maticzav/emma-cli.git"
  }
}
