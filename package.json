{
  "name": "emma-cli",
  "description": "Great assistant who helps you search Yarn and NPM in your terminal.",
  "author": "Matic Zavadlal <matic.zavadlal@gmail.com>",
  "version": "0.0.0-semantic-release",
  "bin": {
    "emma": "dist/index.js"
  },
  "release": {
    "branch": "master"
  },
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist",
    "prepublish": "npm run build",
    "dev": "babel -w src --out-dir dist",
    "test": "prettier-check && ava",
    "pretest": "npm run build",
    "semantic-release": "semantic-release"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/maticzav/emma-cli",
  "keywords": [
    "emma",
    "cli",
    "search",
    "packages",
    "npm",
    "yarn",
    "algolia",
    "install"
  ],
  "bugs": {
    "url": "https://github.com/maticzav/emma-cli/issues"
  },
  "dependencies": {
    "algoliasearch": "^3.27.0",
    "execa": "^0.9.0",
    "has-ansi": "^3.0.0",
    "ink": "^0.4.1",
    "ink-spinner": "^1.0.0",
    "meow": "^4.0.0",
    "prop-types": "^15.6.1",
    "update-notifier": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-jsx": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.40",
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.3",
    "cross-env": "^5.1.3",
    "eslint": "^4.19.1",
    "eslint-plugin-prettier": "^2.6.0",
    "prettier": "^1.12.1",
    "prettier-check": "^2.0.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^12.4.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maticzav/emma-cli.git"
  }
}
