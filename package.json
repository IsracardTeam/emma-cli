{
  "name": "emma-cli",
  "description": "Great assistant who helps you search Yarn and NPM in your terminal.",
  "author": "Matic Zavadlal <matic.zavadlal@gmail.com>",
  "version": "0.0.0-semantic-release",
  "bin": {
    "emma": "dist/index.js"
  },
  "release": {
    "branch": "master"
  },
  "scripts": {
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "build": "rm -rf dist && tsc -d",
    "dev": "nodemon -e ts -x ts-node --inspect src/index.ts",
    "lint": "tslint --project tsconfig.json {src,test}/**/*.ts && prettier-check --ignore-path .gitignore {src,.}/{*.ts,*.js}",
    "pretest": "npm run build",
    "semantic-release": "semantic-release"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/maticzav/emma-cli",
  "keywords": [
    "emma"
  ],
  "bugs": {
    "url": "https://github.com/maticzav/emma-cli/issues"
  },
  "dependencies": {
    "algoliasearch": "^3.24.12",
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link": "^1.2.1",
    "apollo-link-http": "^1.5.3",
    "apollo-link-ws": "^1.0.7",
    "apollo-utilities": "^1.0.11",
    "conf": "^1.4.0",
    "dot-prop": "^4.2.0",
    "execa": "^0.9.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "ink": "^0.4.1",
    "ink-link": "^0.2.0",
    "ink-spinner": "^1.0.0",
    "ink-text-input": "^1.1.1",
    "isomorphic-fetch": "^2.2.1",
    "meow": "^4.0.0",
    "opn": "^5.3.0",
    "subscriptions-transport-ws": "^0.9.7",
    "term-size": "^1.2.0",
    "update-notifier": "^2.3.0",
    "ws": "^5.1.1"
  },
  "devDependencies": {
    "@types/algoliasearch": "^3.24.9",
    "@types/conf": "^1.4.0",
    "@types/execa": "^0.9.0",
    "@types/graphql": "^0.13.0",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/meow": "^4.0.1",
    "@types/node": "^9.6.5",
    "@types/react": "^16.3.8",
    "@types/update-notifier": "^2.2.0",
    "@types/ws": "^4.0.2",
    "ava": "^0.25.0",
    "cross-env": "^5.1.3",
    "nodemon": "^1.17.3",
    "prettier": "^1.11.1",
    "prettier-check": "^2.0.0",
    "react": "^16.3.1",
    "rimraf": "^2.6.2",
    "semantic-release": "^12.4.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-eslint-rules": "^5.1.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maticzav/emma-cli.git"
  }
}
